# Wallpaper
nitrogen --restore &

# DWM Status
dwmbar &

# Notification client
dunst &

# The compositor
picom &

# Startup application
discord &
spotify &
telegram-desktop &

while true; do
    # Log stderror to a file 
    dwm 2> ~/.dwm.log
    # No error logging
    dwm >/dev/null 2>&1
done

# Execute dwm
exec dwm
